{
  "dns": {
    "final": "google-dns",
    "reverse_mapping": true,
    "rules": [
      {
        "action": "reject",
        "method": "default",
        "query_type": [
          "AAAA"
        ]
      },
      {
        "action": "route",
        "domain_suffix": [
          ".cn"
        ],
        "server": "local-dns"
      }
    ],
    "servers": [
      {
        "detour": "proxy",
        "server": "dns.google",
        "tag": "google-dns",
        "type": "https"
      },
      {
        "detour": "direct",
        "server": "223.5.5.5",
        "tag": "local-dns",
        "type": "udp"
      }
    ],
    "strategy": "prefer_ipv4"
  },
  "inbounds": [
    {
      "address": [
        "172.18.0.1/30",
        "fdfe:dcba:9876::1/126"
      ],
      "auto_route": true,
      "route_exclude_address": [
        "144.202.10.170/32",
        "223.5.5.5/32"
      ],
      "strict_route": false,
      "tag": "tun-in",
      "type": "tun"
    }
  ],
  "log": {
    "level": "info"
  },
  "outbounds": [
    {
      "method": "aes-256-gcm",
      "multiplex": {
        "enabled": false
      },
      "password": "yourpassword123",
      "server": "144.202.10.170",
      "server_port": 10086,
      "tag": "proxy",
      "type": "shadowsocks"
    },
    {
      "domain_strategy": "prefer_ipv4",
      "tag": "direct",
      "type": "direct"
    }
  ],
  "route": {
    "auto_detect_interface": true,
    "default_domain_resolver": "local-dns",
    "final": "direct",
    "rules": [
      {
        "action": "sniff"
      },
      {
        "action": "hijack-dns",
        "protocol": "dns"
      }
    ]
  }
}
